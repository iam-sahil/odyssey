{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "demo-primitives-animate-cursor",
  "type": "registry:ui",
  "title": "Animate Cursor Demo",
  "description": "Demo showing an animated cursor.",
  "registryDependencies": ["@animate-ui/primitives-animate-cursor"],
  "files": [
    {
      "path": "registry/demo/primitives/animate/cursor/index.tsx",
      "type": "registry:ui",
      "target": "components/animate-ui/demo/primitives/animate/cursor.tsx"
    }
  ],
  "meta": {
    "demoProps": {
      "CursorProvider": {
        "global": {
          "value": false
        },
        "enableCursor": {
          "value": true
        },
        "enableCursorFollow": {
          "value": true
        }
      },
      "CursorFollow": {
        "side": {
          "value": "bottom",
          "options": {
            "top": "top",
            "right": "right",
            "bottom": "bottom",
            "left": "left"
          }
        },
        "sideOffset": {
          "value": 15
        },
        "align": {
          "value": "end",
          "options": {
            "start": "start",
            "center": "center",
            "end": "end"
          }
        },
        "alignOffset": {
          "value": 5
        }
      }
    }
  }
}
